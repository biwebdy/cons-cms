{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "client",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "client_id": {
      "type": "string",
      "required": false,
      "unique": false
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "required": true
    },
    "street": {
      "type": "string"
    },
    "number": {
      "type": "string"
    },
    "areaPostalCodes": {
      "type": "string"
    },
    "commune": {
      "type": "string"
    },
    "canton": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::canton.canton"
    },
    "country": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "details": {
      "type": "text"
    },
    "accountOwnerFirstName": {
      "type": "string"
    },
    "accountOwnerLastName": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos",
        "audios"
      ]
    },
    "attachment": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "client"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "client"
    },
    "invoices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice.invoice",
      "mappedBy": "client"
    },
    "proposals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::proposal.proposal",
      "mappedBy": "client"
    },
    "phoneNumber": {
      "type": "text"
    },
    "deletedUserData": {
      "type": "json",
      "private": true
    },
    "isDeleted": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "parentClient": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::client.client",
      "private": false
    }
  }
}
